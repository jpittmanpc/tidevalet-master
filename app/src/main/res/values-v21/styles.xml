<resources>>
    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
    </style>

    <style name="TideValet" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/TideBlue</item>
        <item name="colorPrimaryDark">@color/TideBlueDark</item>
        <item name="colorAccent">#7ea538</item>
        <item name="android:colorBackground">#e2e3e4</item>
        <item name="android:colorForeground">@color/button_material_dark</item>
        <item name="android:navigationBarColor">#000000</item>
        <item name="android:statusBarColor">@color/TideBlueDark</item>
        <item name="android:textColorPrimary">@color/TideBlue</item>
        <item name="android:textColorPrimaryInverse">#000000</item>
        <item name="android:textColorSecondary">@color/TideBlue</item>
        <item name="android:textColorSecondaryInverse">@color/dkgrey</item>
        <item name="android:windowBackground">@color/dkgrey</item>
    </style>
    <color name="dkgrey">#e2e3e4</color>

</resources>
